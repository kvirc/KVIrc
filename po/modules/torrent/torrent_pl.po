#: src/modules/torrent/KTorrentDbusInterface.cpp:40
msgid ""
"An interface to KDE's excellent KTorrent client.\n"
"Download it from http://www.ktorrent.org\n"
msgstr "Interfejs dla pochodzącego z KDE wspaniałego klienta Ktorrent.\nPobierz go z http://www.ktorrent.org\n"

#: src/modules/torrent/libkvitorrent.cpp:62
msgid "Trying torrent client interface \"%Q\": score %d"
msgstr "Próbowanie interfejsu klienta torrent \"%Q\": wynik %d"

#: src/modules/torrent/libkvitorrent.cpp:102
msgid "Choosing torrent client interface \"%Q\""
msgstr "Wybieranie interfejsu klienta torrent \"%Q\""

#: src/modules/torrent/libkvitorrent.cpp:109
msgid "Seems that there is no usable torrent client on this machine"
msgstr "Wygląda na to, że nie ma użytecznego klienta torrent na tej maszynie"

#: src/modules/torrent/libkvitorrent.cpp:118
msgid "No torrent client interface selected. Try /torrent.detect"
msgstr "Nie wybrano interfejsu klienta torrent. Spróbuj /torrent.detect"

#: src/modules/torrent/libkvitorrent.cpp:128
msgid ""
"The selected torrent client interface failed to execute the requested "
"function"
msgstr "Wybranemu interfejsowi klienta torrent nie udało się wykonać żądanej funkcji"

#: src/modules/torrent/libkvitorrent.cpp:129
msgid "Last interface error: "
msgstr "Ostatni błąd interfejsu:"

#: src/modules/torrent/libkvitorrent.cpp:714
msgid "Using client interface \"%Q\"."
msgstr "Użycie interfejsu klienta \"%Q\"."

#: src/modules/torrent/libkvitorrent.cpp:722
msgid "Invalid client interface \"%Q\"!"
msgstr "Niepoprawny interfejs klienta \"%Q\"!"

